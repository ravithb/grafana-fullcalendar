{
  "name": "kr-fullcalendar-panel",
  "version": "1.0.0",
  "description": "A full calendar panel based on fullcalendar.io",
  "main": "src/module.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "lint": "eslint --color .",
    "lint:watch": "onchange 'src/**/*.js' -- npm run lint",
    "build": "grunt",
    "prebuild": "npm run clean",
    "build:watch": "onchange './src/**/*.js' './src/**/*.css' './src/**/*.html' './src/plugin.json' -- npm run build"
  },
  "keywords": [
    "fullcalendar.io",
    "grafana",
    "plugin",
    "panel"
  ],
  "author": "Ravith Botejue",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.14.0",
    "grunt": "^1.0.3",
    "grunt-babel": "^8.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-execute": "^0.2.2",
    "grunt-systemjs-builder": "^1.0.0",
    "load-grunt-tasks": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ravithb/grafana-fullcalendar.git"
  }
}
